<div>
    <a href="<%= root_path %>">Go HOME</a>
</div>


<%= form_for @cohort do |f| %>

<div>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
</div>
   
   
   <div>
    <%= f.label :start_date %>
    <%= f.date_field :start_date %>
    
</div>
  
  
   <div>
    <%= f.label :end_date %>
    <%= f.date_field :end_date %>
    
</div>
 
  
<!--
   <div>
    <= f.label :course_id %>
    <= f.number_field :course_id %>
        
    
  </div>
-->
 
  <div>
    <%= f.label :course_id %>
    <%= f.select :course_id, options_for_select(Course.all.map {|course| [course.name, course.id]}) %>
    
  </div>
 
  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
    
</div>
  


<div>
    <%= f.submit  %>
</div>

<% end %>
